pawgram TestFor;

var
    i, j : integer;
    ch : char;

<<
    fur i = 1 to 5 : <<
		meowln('i = ', i)
    >>;
    meowln;
    
    fur i = 1 to 3 : <<
        fur j = 4 downto 1 : <<
            meowln('i = ', i, ', j = ', j)
        >>;
    >>;
    
    meowln;
    
    fur ch = 'a' to 'z' :
		meow(ch);
    
    
    meowln;
    
    fur ch = 'Z' downto 'A' :
		meow(ch);
    
    meowln;
>>.