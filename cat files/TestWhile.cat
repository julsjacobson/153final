pawgram TestWhile;

var 
    i, j : integer;


<<
    i = 1;
    while i <= 5 <<
        meowln('i = ', i);
        i = i + 1
    >>;
    
    meowln;
    
    i = 1;
    while i <= 5 <<
        j = 10;
        
        while j <= 30 <<
            meowln('i = ', i, ', j = ', j);
            j = j + 10
        >>;
        
        i = i + 1
    >>
>>.






